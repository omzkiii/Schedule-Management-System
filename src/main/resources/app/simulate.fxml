<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.AppController">
   <children>
      <VBox alignment="CENTER" prefHeight="90.0" prefWidth="460.0">
         <children>
            <Label prefHeight="42.0" prefWidth="110.0" text="Simulate" textAlignment="CENTER">
               <font>
                  <Font name="Kristen ITC" size="25.0" />
               </font>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
      <VBox prefHeight="303.0" prefWidth="430.0">
         <children>
            <TableView fx:id="simList" prefHeight="301.0" prefWidth="420.0" stylesheets="@../../java/app/style.css">
               <columns>
                  <TableColumn prefWidth="75.0" text="Room" />
                  <TableColumn prefWidth="75.0" text="Faculty" />
                  <TableColumn prefWidth="75.0" text="Section" />
                  <TableColumn prefWidth="75.0" text="Start" />
                  <TableColumn prefWidth="75.0" text="End" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </TableView>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
